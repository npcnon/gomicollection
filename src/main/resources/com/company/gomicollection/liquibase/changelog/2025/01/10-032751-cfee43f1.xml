<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="gomicollection">
        <addColumn tableName="EMPLOYEE">
            <column name="ADDRESS" type="VARCHAR(255)"/>
        </addColumn>

        <addNotNullConstraint columnName="ADDRESS" defaultNullValue="''" tableName="EMPLOYEE"/>
    </changeSet>
    <changeSet id="2" author="gomicollection">
        <addColumn tableName="EMPLOYEE">
            <column name="AGE" type="INT"/>
        </addColumn>

        <addNotNullConstraint columnName="AGE" defaultNullValue="0" tableName="EMPLOYEE"/>
    </changeSet>
    <changeSet id="3" author="gomicollection">
        <addColumn tableName="EMPLOYEE">
            <column name="CONTACT_NUMBER" type="VARCHAR(255)"/>
        </addColumn>

        <addNotNullConstraint columnName="CONTACT_NUMBER" defaultNullValue="''" tableName="EMPLOYEE"/>
    </changeSet>
    <changeSet id="4" author="gomicollection">
        <addColumn tableName="EMPLOYEE">
            <column name="EMAIL" type="VARCHAR(255)"/>
        </addColumn>

        <addNotNullConstraint columnName="EMAIL" defaultNullValue="''" tableName="EMPLOYEE"/>
    </changeSet>
    <changeSet id="5" author="gomicollection">
        <addColumn tableName="EMPLOYEE">
            <column name="FIRST_NAME" type="VARCHAR(255)"/>
        </addColumn>

        <addNotNullConstraint columnName="FIRST_NAME" defaultNullValue="''" tableName="EMPLOYEE"/>
    </changeSet>
    <changeSet id="6" author="gomicollection">
        <addColumn tableName="EMPLOYEE">
            <column name="LAST_NAME" type="VARCHAR(255)"/>
        </addColumn>

        <addNotNullConstraint columnName="LAST_NAME" defaultNullValue="''" tableName="EMPLOYEE"/>
    </changeSet>
    <changeSet id="7" author="gomicollection">
        <addColumn tableName="EMPLOYEE">
            <column name="MIDDLE_NAME" type="VARCHAR(255)"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>